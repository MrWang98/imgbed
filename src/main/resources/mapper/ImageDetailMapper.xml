<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.demo.imgbed.mapper.ImageDetailMapper">
    <select id="selectByUserAndFilename" resultType="cn.demo.imgbed.entity.ImageDetail">
        SELECT * FROM user_img WHERE filename=#{filename} and ownerid=#{id}
    </select>

    <select id="selectById" resultType="cn.demo.imgbed.entity.ImageDetail">
        SELECT * FROM user_img WHERE ownerid=#{id}
    </select>
</mapper>
